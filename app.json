{
  "expo": {
    "name": "Ramonline",
    "slug": "ramonline",
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/ramonline-icon.png",
    "scheme": "ramonline",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ramonline-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.prmsu.ramonline",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ramonline-logo.png",
          "imageWidth": 256,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/ramonline-logo.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ebbec764-fc88-4eff-a3eb-7eefe08e96e5"
      }
    },
    "owner": "prmsu"
  }
}
